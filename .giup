{
	"merge-paths": [
		"1.18",
		"1.18->1.19"
	],
	"commands": [
		{
			"title": "Cleanup build files",
			"run": "rm -rf build .gradle",
			"nt": "rmdir /S /Q build & rmdir /S /Q .gradle",
			"ignore-errors": true
		},
		{
			"title": "Publish",
			"run": "./gradlew publish",
			"nt": "gradlew publish"
		},
		{
			"title": "Push Changes",
			"run": "git push"
		},
		{
			"title": "Upload",
			"run": "./gradlew uploadToModSites",
			"nt": "gradlew uploadToModSites"
		}
	]
}
